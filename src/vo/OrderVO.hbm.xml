<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="vo.OrderVO" table="order_table">
  
  
  <id name="o_id" type="int" column="o_id" >
   <generator class="increment"/>
  </id>
  
 <property name="card_no">
   <column name="card_no" />
  </property>
   <property name="cart_name">
   <column name="cart_name" />
  </property>
  <property name="paid_amount">
   <column name="paid_amount" />
  </property>
 
   
  
  
   </class>
  
</hibernate-mapping>
